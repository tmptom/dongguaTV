{
  "rewrites": [
    {
      "source":"/api/:path*",
      "destination": "index.js"
    },
    {
      "source": "/s=:query",
      "destination": "/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/admin.html/:path*",
      "destination": "/admin.html"
    }
  ]
}
